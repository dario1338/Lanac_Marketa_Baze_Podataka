            <TextField fx:id="sifra" prefHeight="50.0" prefWidth="305.0" promptText="Sifra proizvoda" />
            <TextField fx:id="naziv" prefHeight="50.0" prefWidth="305.0" promptText="Naziv proizvoda" />
