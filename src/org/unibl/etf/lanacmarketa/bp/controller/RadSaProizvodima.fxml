            <Insets top="30.0" />
         </padding>
      </VBox>
      <TableView fx:id="proizvodTableView" layoutX="238.0" prefHeight="424.0" prefWidth="534.0">
        <columns>
          <TableColumn fx:id="sifraProizvoda" prefWidth="137.0" text="Sifra" />
          <TableColumn fx:id="nazivProizvoda" prefWidth="257.0" text="Naziv" />
            <TableColumn fx:id="cijenaProizvoda" minWidth="0.0" prefWidth="139.0" text="Cijena" />
        </columns>
      </TableView>
   </children>
</AnchorPane>
